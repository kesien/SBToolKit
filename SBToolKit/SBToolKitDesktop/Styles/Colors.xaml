<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="ColorGrey">#2B2D42</Color>
    <Color x:Key="ColorDarkGrey">#181925</Color>
    <Color x:Key="ColorLightGrey">#8D99AE</Color>
    <Color x:Key="ColorWhite">#EDF2F4</Color>
    <Color x:Key="ColorRed">#EF233C</Color>
    <Color x:Key="ColorDarkRed">#D90429</Color>
    <Color x:Key="ColorOrange">#ff7900</Color>
    <Color x:Key="ColorDarkOrange">#ff6000</Color>

    <SolidColorBrush x:Key="ColorGreyBrush" Color="{StaticResource ColorGrey}"/>
    <SolidColorBrush x:Key="ColorDarkGreyBrush" Color="{StaticResource ColorDarkGrey}"/>
    <SolidColorBrush x:Key="ColorLightGreyBrush" Color="{StaticResource ColorLightGrey}"/>
    <SolidColorBrush x:Key="ColorWhiteBrush" Color="{StaticResource ColorWhite}"/>
    <SolidColorBrush x:Key="ColorRedBrush" Color="{StaticResource ColorRed}"/>
    <SolidColorBrush x:Key="ColorDarkRedBrush" Color="{StaticResource ColorDarkRed}"/>
    <SolidColorBrush x:Key="ColorOrangeBrush" Color="{StaticResource ColorOrange}"/>
    <SolidColorBrush x:Key="ColorDarkOrangeBrush" Color="{StaticResource ColorDarkOrange}"/>
</ResourceDictionary>